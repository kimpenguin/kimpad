<%= form_for @chronicle do |f| %>
	<%=f.text_field :title, placeholder: "Title"%><br>
	<%=f.text_area :body, placeholder: "Text"%><br>
	<%=f.text_area :reference_url, placeholder: "URL"%><br>
	<%=f.text_area :image_url, placeholder: "URL"%><br>
	<%= f.hidden_field :user_id, value:current_user.id%>
	<%= f.hidden_field :manual, value:"true"%>
	<%=f.submit %>
<% end %>
